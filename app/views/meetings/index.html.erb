<p id="notice"><%= notice %></p>

<h1>Meetings</h1>

<table class="table">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>City</th>
      <th>User</th>
      <th>Interest</th>
      <th>Date time start</th>
      <th>Date time finish</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetings.each do |meeting| %>
      <tr>
        <td><%= meeting.title %></td>
        <td><%= meeting.description %></td>
        <td><%= meeting.city.title if meeting.city %></td>
        <td><%= meeting.user.name if meeting.user %></td>
        <td><%= meeting.interest.title if meeting.interest %></td>
        <td><%= meeting.date_time_start %></td>
        <td><%= meeting.date_time_finish %></td>
        <td><%= meeting.address %></td>
        <td><%= link_to 'Show', meeting %></td>
        <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
        <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meeting', new_meeting_path %>